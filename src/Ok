                                    'id': 'state'
                                },
                                'define': {
                                    'heading': 'Define',
                                    'id': 'define'
                                },
                                'support': {
                                    'heading': 'Support',
                                    'id': 'support'
                                },
                                'relate': {
                                    'heading': 'Relate',
                                    'id': 'relate'
                                }
                            }
                        }
                    }
                },
                'conclusion': {
                    'heading': 'Conclusion',
                    'id': 'conclusion',
                    'isCollapsed': false,
                    'subCategory': {
                        'brakeLight': {
                            'heading': 'Brake light',
                            'id': 'brakeLight'
                        },
                        'summary': {
                            'heading': 'Summary',
                            'id': 'summary'
                        },
                        'appealToAction': {
                            'heading': 'Appeal to Action/Use',
                            'id': 'appealToAction'
                        }
                    }
                },
                'delivery': {
                    'heading': 'Delivery',
                    'id': 'delivery',
                    'isCollapsed': false,
                    'subCategory': {
                        'conversational': {
                            'heading': 'Conversational',
                            'id': 'conversational'
                        },
                        'rate': {
                            'heading': 'Rate',
                            'id': 'rate'
                        },
                        'volume': {
                            'heading': 'Volume',
                            'id': 'volume'
                        },
                        'eyeContact': {
